@model Lanche
<div class="col-sm-4 col-lg-4 col-md-4">
    <div class="img-thumbnail">
        <img src="@Model.ImagemUrl" class="img-fluid" alt="Responsive image">
    </div>
    <div class="caption">
       <h3>
        @Model.LancheNome
       </h3>
    </div>
        <h3 class="pull-right">@Model.Preco.ToString("c")</h3>
    <p>@Model.DescCurta</p>
</div>